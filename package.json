{
  "name": "aeorank",
  "version": "1.2.0",
  "description": "AI Engine Optimization audit - score any website across 23 criteria for AI visibility",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "aeorank": "./dist/cli.js"
  },
  "files": [
    "dist",
    "data",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aeo",
    "ai",
    "seo",
    "audit",
    "llm",
    "chatgpt",
    "claude",
    "perplexity",
    "ai-visibility",
    "schema-markup",
    "structured-data",
    "llms-txt",
    "github-actions",
    "benchmark"
  ],
  "author": "AEO Content, Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AEO-Content-Inc/aeorank.git"
  },
  "homepage": "https://www.aeocontent.ai",
  "bugs": {
    "url": "https://github.com/AEO-Content-Inc/aeorank/issues"
  },
  "peerDependencies": {
    "puppeteer": ">=21.0.0"
  },
  "peerDependenciesMeta": {
    "puppeteer": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.3.3",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
